{
  "id": "autoscale-mesos",
  "cmd": "python mesos_ec2.py --mesos-url http://10.0.0.223:5050 --log-level info --cpus 1,3 --region eu-west-1 --asg MesosCluster",
  "cpus": 0.2,
  "mem": 64.0,
  "healthChecks": [
    {
      "command": {
        "value": "python --version"
      },
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 3,
      "protocol": "COMMAND",
      "ipProtocol": "IPv4",
      "timeoutSeconds": 20,
      "delaySeconds": 15
    }
  ],
  "networks": [ { "mode": "container/bridge" } ],
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "dockerhub.mago-data.com/autoscale-python:latest"
    }
  }
}